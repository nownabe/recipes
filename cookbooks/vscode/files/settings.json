{
  "workbench.startupEditor": "newUntitledFile",
  "editor.formatOnSave": true,
  "editor.renderWhitespace": "all",
  "editor.formatOnPaste": true,
  "editor.tabSize": 2,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "j"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "eslint.autoFixOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {
      "language": "typescript",
      "autoFix": true
    },
    {
      "language": "typescriptreact",
      "autoFix": true
    }
  ],
  "go.useLanguageServer": true,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "go.languageServerExperimentalFeatures": {
    "format": true,
    "autoComplete": true,
    "rename": true,
    "goToDefinition": true,
    "hover": true,
    "signatureHelp": true,
    "goToTypeDefinition": true,
    "goToImplementation": true,
    "documentSymbols": true,
    "workspaceSymbols": true,
    "findReferences": true,
    "diagnostics": true,
    "documentLink": true
  },
  "gopls": {
    "usePlaceholders": true,
    "completeUnimported": true
  },
  "go.autocompleteUnimportedPackages": true,
  "go.gopath": "/home/nownabe",
  "go.lintOnSave": "workspace",
  "go.vetOnSave": "workspace",
  "go.buildOnSave": "workspace",
  "go.installDependenciesWhenBuilding": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "editor.minimap.enabled": false,
  "extensions.ignoreRecommendations": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "files.trimTrailingWhitespace": true,
  "[yaml]": {},
  "vs-kubernetes": {
    "vs-kubernetes.minikube-path": "/home/nownabe/.vs-kubernetes/tools/minikube/linux-amd64/minikube",
    "vs-kubernetes.minikube-path.linux": "/home/nownabe/.vs-kubernetes/tools/minikube/linux-amd64/minikube",
    "vs-kubernetes.kubectl-path.linux": "/home/nownabe/.vs-kubernetes/tools/kubectl/kubectl",
    "vs-kubernetes.draft-path.linux": "/home/nownabe/.vs-kubernetes/tools/draft/linux-amd64/draft"
  },
  "ruby.rubocop.useBundler": true,
  "ruby.rubocop.executePath": "bundle exec rubocop",
  "window.zoomLevel": 1,
  "textlint.autoFixOnSave": true,
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [
      80
    ],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  }
}